PORT=5000
MONGODB_URI=mongodb://localhost:27017/jmc_enterprises
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRE=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Admin Credentials
ADMIN_EMAIL=admin@jmc.com
ADMIN_PASSWORD=admin123

# Razorpay (Primary for India - UPI, Cards, Wallets)
# Get keys from: https://dashboard.razorpay.com
# Test keys for development:
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=your_test_razorpay_secret_key

# Stripe (International - Credit/Debit Cards)
# Get keys from: https://dashboard.stripe.com
# Test keys for development:
STRIPE_PUBLIC_KEY=pk_test_xxxxxxxxxxxxx
STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxxx

# For production, update with:
# RAZORPAY_KEY_ID=rzp_live_xxxxxxxxxxxxx
# RAZORPAY_KEY_SECRET=your_live_razorpay_secret_key
# STRIPE_PUBLIC_KEY=pk_live_xxxxxxxxxxxxx
# STRIPE_SECRET_KEY=sk_live_xxxxxxxxxxxxx
